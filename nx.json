{
  "tasksRunnerOptions": {
    "default": {
      "options": {}
    }
  },
  "extends": "nx/presets/npm.json",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "affected": {
    "defaultBase": "origin/main"
  },
  "namedInputs": {
    "default": ["{projectRoot}/**/*"],
    "production": [
      "default",
      "!{projectRoot}/**/*.spec.ts",
      "!{projectRoot}/tests/**/*",
      "!{projectRoot}/coverage/**/*",
      "!{projectRoot}/dist/**/*",
      "!{projectRoot}/.next/**/*",
      "!{projectRoot}/.lintstagedrc.cjs",
      "!{projectRoot}/.prettierignore",
      "!{projectRoot}/.env.*"
    ]
  },
  "targetDefaults": {
    "build": {
      "inputs": ["production", "^production"],
      "dependsOn": ["^build"],
      "outputs": ["{projectRoot}/dist"]
    },
    "lint": {
      "inputs": ["default", "^production"],
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": ["^build"]
    },
    "lint:next": {
      "cache": true
    },
    "lint:prettier": {
      "cache": true
    },
    "lint:stylelint": {
      "cache": true
    },
    "lint:tsc": {
      "cache": true
    },
    "test": {
      "cache": true
    },
    "test:ci": {
      "cache": true
    },
    "test:cron": {
      "cache": true
    }
  },
  "nxCloudAccessToken": "Y2M2OWVmODMtNzY3OC00MDFmLTkyMDYtNWQ3YTNkOGE0YmJlfHJlYWQ="
}
